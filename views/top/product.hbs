<div class="col-sm">
    <div class="card">
        <div class="card-header">
            <strong class="card-title">Top 10 điện thoại bán chạy</strong>
        </div>
        <div class="card-body">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">Mã sản phẩm</th>
                        <th scope="col">Thương hiệu</th>
                        <th scope="col">Tên sản phẩm</th>
                        <th scope="col">Số lượng bán ra</th>
                    </tr>
                </thead>
                <tbody>
                    {{#if data}}
                    {{#each data}}
                    <tr>
                        <th scope="row">{{this.id}}</th>
                        <td>{{this.brand}}</td>
                        <td>{{this.name}}</td>
                        <td>{{this.count}}</td>
                    </tr>
                    {{/each}}
                    {{/if}}
                </tbody>
            </table>
        </div>
    </div>
</div>