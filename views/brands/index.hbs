<div class="col-sm">
    <div class="card">
        <div class="card-header">
            <strong class="card-title">Danh sách</strong>
        </div>
        <div class="card-body">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col" style="width: 25%">Mã</th>
                        <th scope="col" style="width: 25%">Tên</th>
                        <th scope="col" style="width: 25%">Số lượng sản phẩm</th>
                        <th scope="col" style="width: 25%">Đã bán</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each data}}
                    <tr>
                        <th scope="row">{{this._id}}</th>
                        <td>{{this.name}}</td>
                        <td>{{this.count}}</td>
                        <td>{{this.sold}}</td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>